{
  "name": "soybean-admin",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "dev:prod": "vite --mode production",
    "dev:staging": "vite --mode staging",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
