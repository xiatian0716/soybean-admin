    "clipboard": "^2.0.8",
    "dayjs": "^1.10.7",
    "element-plus": "^1.2.0-beta.3",
    "form-data": "^4.0.0",
    "naive-ui": "^2.20.3",
    "pinia": "^2.0.3",
    "qs": "^6.10.1",
    "swiper": "^7.2.0",
    "vditor": "^3.8.7",
    "vue": "^3.2.20",
    "vue-router": "^4.0.11",
    "wangeditor": "^4.7.9",
    "xgplayer": "^2.31.2"
