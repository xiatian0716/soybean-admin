    "prettier": "^2.4.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "sass": "^1.43.4",
    "typescript": "^4.4.4",
    "unplugin-icons": "^0.12.18",
    "unplugin-vue-components": "^0.17.2",
